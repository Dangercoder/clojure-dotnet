(ns myapp.core
  (:require [mylib.utils :as utils]
            [mylib.math :as math :refer [sum-squared]]))

;; Main application that uses both libraries

(defn -main
  "Main entry point"
  [& args]
  (println (utils/greet "World"))
  (println "Quadruple of 5:" (math/quadruple 5))
  (println "Sum of squares 3,4:" (sum-squared 3 4)))
